"""
Content mock data
"""

 INSERT INTO content (athlete_id, media_url, caption) SELECT    FLOOR(RAND() * 42) + 1 as athlete_id,   CONCAT('http://example.com/image', FLOOR(RAND() * 1000) + 1, '.jpg') as media_url,   CONCAT(     CASE FLOOR(RAND() * 3)       WHEN 0 THEN 'Just dropped some new merch! Check it out at {merch_url}.'       WHEN 1 THEN 'Training hard for the upcoming {event_name}! Can\'t wait to compete.'       ELSE 'So excited to announce that I\'m partnering with {brand_name}! Stay tuned for more details.'     END,     ' 😍 #sportslife #newbeginnings',     FLOOR(RAND() * 1000) + 1   ) as caption FROM information_schema.tables AS t1   JOIN information_schema.tables AS t2   JOIN information_schema.tables AS t3 LIMIT 50;


